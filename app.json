{
  "name": "Downloader",
  "description": "M3U8 Downloader for TG",
  "repository": "https://github.com/MalithCrpt/M3U8_downloader",
  "env": {
    "BOT_TOKEN": {
      "description": "Your bot token",
      "required": true
    },
    "API_ID": {
      "description": "Your API ID",
      "required": true
    },
    "API_HASH": {
      "description": "Your API hash",
      "required": true
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql"
    }
  ],
  "buildpacks": [{
    "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest"
  }, {
    "url": "heroku/python"
  }]
}
